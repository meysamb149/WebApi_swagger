<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="WebApi_swagger" namespace="WebApi_swagger.Models" xmlns="urn:nhibernate-mapping-2.2">
  <class name="TUserRol" table="T_User_Rol"  lazy="true" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="IdUserRol" column="ID_User_Rol">
      <generator class="native" />
    </id>
    <property name="RolTitels">
      <column name="Rol_Titels" sql-type="nvarchar" not-null="false" />
    </property>
    <bag name="TLaw" inverse="true">
      <key column="User_Rol_ID" />
      <one-to-many class="TLaw" />
    </bag>
  </class>
</hibernate-mapping>