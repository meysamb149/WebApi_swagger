<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="WebApi_swagger" namespace="WebApi_swagger.Models" xmlns="urn:nhibernate-mapping-2.2">
  <class name="TAddresses" table="T_Addresses" lazy="true" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="IdAddresses" column="ID_Addresses">
      <generator class="native" />
    </id>
    <many-to-one name="TUsers">
      <column name="Users_ID" sql-type="bigint" not-null="false" />
    </many-to-one>
    <many-to-one name="TLMahaleh">
      <column name="Mahaleh_ID" sql-type="int" not-null="false" />
    </many-to-one>
    <many-to-one name="TLCity">
      <column name="City_ID" sql-type="int" not-null="false" />
    </many-to-one>
    <many-to-one name="TLOstan">
      <column name="Ostan_ID" sql-type="int" not-null="false" />
    </many-to-one>
    <many-to-one name="TLActive">
      <column name="Active_ID" sql-type="int" not-null="false" />
    </many-to-one>
    <property name="AddressName">
      <column name="Address_Name" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Titels_Address">
      <column name="Titels_Address" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Latitude">
      <column name="Latitude" sql-type="decimal" not-null="false" />
    </property>
    <property name="Longitude">
      <column name="Longitude" sql-type="decimal" not-null="false" />
    </property>
    <property name="Phone_Addresss">
      <column name="Phone_Addresss" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Lastorderid">
      <column name="LastOrderID" sql-type="bigint" not-null="false" />
    </property>
    <bag name="TOrder" inverse="true">
      <key column="Address_ID" />
      <one-to-many class="TOrder" />
    </bag>
    <bag name="TTemporder" inverse="true">
      <key column="Address_ID" />
      <one-to-many class="TTemporder" />
    </bag>
  </class>
</hibernate-mapping>