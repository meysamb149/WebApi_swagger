<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="WebApi_swagger" namespace="WebApi_swagger.Models" xmlns="urn:nhibernate-mapping-2.2">
  <class name="TGroupProduct" table="T_Group_product" lazy="true" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="IdGroupProduct" column="ID_Group_product">
      <generator class="native" />
    </id>
    <many-to-one name="TGroup">
      <column name="Group_ID" sql-type="int" not-null="false" />
    </many-to-one>
    <property name="GroupProductTitels">
      <column name="Group_product_Titels" sql-type="nvarchar" not-null="false" />
    </property>
    <bag name="TNoeProduct" inverse="true">
      <key column="Group_product_ID" />
      <one-to-many class="TNoeProduct" />
    </bag>
  </class>
</hibernate-mapping>